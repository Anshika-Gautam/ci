type: object
properties:
  name:
    type: string
  description:
    type: string
  keywords:
    type: array
  version:
    type: string
    pattern: "^(?:0|[1-9]\d*)\.(?:0|[1-9]\d*)\.(?:0|[1-9]\d*)(?:-[\da-z\-]+(?:\.[\da-z\-]+)*)?(?:\+[\da-z\-]+(?:\.[\da-z\-]+)*)?$"
  author:
    type: string
  email:
    type: string
    format: email
required:
  - name
  - version
  - author
